; internal keyboard code in Accu
KEYDEFP = $79

    tay
    txa:pha
    ldx PORTB           ; turn on OS
    cpy #$C0
    bcs ignore

    txa
    ora #%00000001
    sta PORTB

    lda (KEYDEFP),y     ; use system keyboard mapping
    bmi ignore          ; no invers & control keys
    cmp #31
    bpl ok              ; only alphanumeric keys

ignore:
    lda #0
ok:
    sta Result
    stx PORTB           ; restore old PORTB
    pla:tax

    rts
