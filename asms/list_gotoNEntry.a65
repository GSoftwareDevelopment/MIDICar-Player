gotoNEntry:
    txa:pha

    lda #<LIST_ADDR
    sta fileList
    lda #>LIST_ADDR
    sta fileList+1

    jmp checkNEntry

loop:
    jsr nextEntry
    beq exitGotoNEntry

    lda nEntry
    sne
    dec nEntry+1
    dec nEntry

checkNEntry:
    lda nEntry
    ora nEntry+1
    bne loop

exitGotoNEntry:

    pla:tax
