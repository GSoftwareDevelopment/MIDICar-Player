ptrreg = $88;

  txa:pha

  ldy #0
  lda (ptrreg),y
  add #1
  sta fnlen

  ldy #0
  lda MAIN.MIDFILES._bank
  sta (pls),y
  iny
  lda MAIN.MIDFILES._adr
  sta (pls),y
  iny
  lda MAIN.MIDFILES._adr+1
  sta (pls),y
  iny

  lda ptrreg
  sta eptr
  lda ptrreg+1
  sta eptr+1

  ldx #1
loop:
  lda eptr:$2000,x
  sta (pls),y
  iny
  inx
  cpx fnlen:#00
  bmi loop

storeEOFN:
  lda #$9B
  sta (pls),y
  iny

  tya
  add pls
  sta pls
  scc:inc pls+1

  inc totalFiles

  pla:tax
