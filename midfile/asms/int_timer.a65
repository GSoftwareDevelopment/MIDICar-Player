    lda _timerStatus
    bmi skip

doSubTick:
    beq doMainTick
    cmp _subCnt
    beq zeroSubTick

    inc _subCnt
    pla:rti

zeroSubTick:
    lda #0
    sta _subCnt

doMainTick:
    lda _timerTick
    add _tickStep
    sta _timerTick
    bcc skip
    inc _timerTick+1
    bne skip
    inc _timerTick+2

skip:
  pla
